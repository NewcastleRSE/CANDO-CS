#ifndef __Algorithms_H
#define __Algorithms_H
/* MODULE Algorithms */

typedef union
{
	struct
	{
		unsigned char phaseCond;
		unsigned char gain;
		unsigned char f;
		unsigned char thr;
		unsigned short setIntVal;
		unsigned short kernelLen;
	} phaseShift;

	// More algorithms can be added here
} algorithm;

typedef struct
{
	unsigned char		algoOption;
	unsigned char 		phaseOption;
	unsigned char 		gainOption;
	unsigned char 		dutyCycle;
	unsigned char 		manLED;
	unsigned short 		onTime;
	unsigned short 		offTime;
	unsigned short 		manLEDlvl;
	algorithm			algorithm;
} GUI;

/* END Algorithms */
#endif
