/* MODULE Events */

#include "Algorithm.h"

void original_algo()
{
	if(set_inten == 0)
	{
		reg4 = convFloatInt(rconv)*scale;               // Outputs scaling to fit DAC

		if(reg4 >= threshold)                          	// Set intensity process
		{
			if (reg4 >= 4000)
			{
				reg4 = 4000;
			}
		}
		else
		{
			reg4 = 0;
		}
	}
}
